load("@rules_rust//crate_universe:defs.bzl", "crate", "crates_vendor")

crates_vendor(
    name = "crates_vendor",
    annotations = {},
    mode = "local",
    packages = {
        # Macro dependencies
        "bon": crate.spec(
            package = "bon",
            version = "3.3",
        ),
        # Regular dependencies
        "serde": crate.spec(
            features = ["derive"],
            package = "serde",
            version = "1.0",
        ),
        "tokio": crate.spec(
            default_features = False,
            features = [
                "macros",
                "net",
                "rt-multi-thread",
                "signal",
                "time",
            ],
            package = "tokio",
            version = "1.42",
        ),
        "tonic": crate.spec(
            features = ["transport"],
            package = "tonic",
            version = "0.12",
        ),
        "tonic-health": crate.spec(
            default_features = False,
            features = ["transport"],
            package = "tonic-health",
            version = "0.12",
        ),
        "warp": crate.spec(
            package = "warp",
            version = "0.3",
        ),
    },
    repository_name = "queng_vendored",
    tags = ["manual"],
)
